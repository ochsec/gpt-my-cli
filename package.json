  "bugs": {
    "url": "https://github.com/ochsec/gpt-my-cli/issues"
  },
  "homepage": "https://github.com/ochsec/gpt-my-cli#readme",
  "dependencies": {
    "@extractus/article-extractor": "^7.3.1",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.3.1",
    "gpt-tokens": "^1.1.2",
    "openai": "^3.3.0",
    "turndown": "^7.1.2"
  }
}
